{
  "name": "react-frost-core",
  "version": "0.0.27",
  "description": "The library of core frost components.",
  "main": "dist/react-frost-core.min.js",
  "style": "dist/react-frost-core.min.css",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build-app-dev": "webpack-dev-server --config webpack.app.config.js",
    "build-app-prod": "NODE_ENV=production webpack --config webpack.app.config.js",
    "build-dist": "npm run build-dist-dev && npm run build-dist-prod",
    "build-dist-dev": "webpack --config webpack.dist.config.js",
    "build-dist-prod": "NODE_ENV=production webpack --config webpack.dist.config.js",
    "deploy": "npm run build-app-prod && gh-pages -d public",
    "postpublish": "codecov && npm run deploy",
    "prepublishOnly": "npm run build-dist",
    "test": "lintly app src && flow && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dogma-io/react-frost-core.git"
  },
  "keywords": [
    "core",
    "component",
    "components",
    "frost",
    "library",
    "react",
    "ui",
    "widget",
    "widgets"
  ],
  "author": "Matthew Dahl <https://github.com/dogma-io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dogma-io/react-frost-core/issues"
  },
  "homepage": "https://github.com/dogma-io/react-frost-core#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.49",
    "@babel/plugin-transform-react-jsx": "7.0.0-beta.49",
    "babel-core": "^7.0.0-0",
    "babel-jest": "23.0.1",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-auto-css-modules": "1.1.0",
    "babel-plugin-react-code-block": "0.2.0",
    "canvas-prebuilt": "1.6.5-prerelease.1",
    "codecov": "3.0.2",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "flow-bin": "0.73.0",
    "gh-pages": "1.1.0",
    "jest": "23.0.1",
    "lintly": "0.2.0",
    "nodely": "0.4.0",
    "postcss": "6.0.22",
    "postcss-cli": "5.0.0",
    "postcss-cssnext": "3.1.0",
    "postcss-for": "2.1.1",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.5",
    "postcss-mixins": "6.2.0",
    "prismjs": "1.14.0",
    "react-router-dom": "4.2.2",
    "style-loader": "0.21.0",
    "svg-spritemap-webpack-plugin": "2.5.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.9.1",
    "webpack-cli": "2.1.4",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "grammatic": "0.0.2",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-domlistener": "^1.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  }
}
